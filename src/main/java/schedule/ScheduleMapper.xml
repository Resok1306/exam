<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="schedule.ScheduleMapper">

<insert id="save" parameterType="schedule.User">
insert into users(id, name, username,email,phone,website) 
values(#{id},#{name},#{username},#{email},#{phone},#{website})
</insert>

<select id="findById" resultType="schedule.User">
select * from users where id=#{id}
</select>

</mapper>